.user-setting-bind{
    background-color: #F8F8F8;
    padding-bottom: 160px;
}
.user-setting-bind .icon{
    display: flex;
    justify-content: center;
    align-items: center;
}
.user-setting-bind .icon image{
    width: 30px;
    height: 30px;
    border-radius: 100px;
}

.user-setting-bind fa-panel{
    display: block;
}
.user-setting-bind .footer{
    padding: 15px;
    font-size: 12px;
    color: #999999;
}
.user-setting-bind .footer text{
    display: block;
}