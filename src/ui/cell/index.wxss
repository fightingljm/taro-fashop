.fa-cell {
    position: relative;
    padding: 12px 15px;
    display: flex;
    align-items: center;
    line-height: 1.4;
    font-size: 14px
}

.fa-cell::after {
    content: '';
    position: absolute;
    top: 0;
    width: 200%;
    height: 200%;
    -webkit-transform: scale(.5);
    transform: scale(.5);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    pointer-events: none;
    box-sizing: border-box;
    border: 0 solid #e5e5e5;
    border-bottom-width: 1px;
    left: 15px;
    right: 0
}

.fa-cell .fa-cell__icon {
    margin-right: 5px
}

.fa-cell .fa-cell__icon:empty {
    display: none
}

.fa-cell__bd {
    flex: 1
}

.fa-cell__text {
    line-height: 24px;
    font-size: 14px
}

.fa-cell__desc {
    line-height: 1.2;
    font-size: 12px;
    color: #666
}

.fa-cell__ft {
    position: relative;
    text-align: right;
    color: #666
}

.fa-cell__no-pading {
    padding: 0
}

.fa-cell__no-pading .fa-cell__bd_padding {
    padding: 12px 0 12px 15px
}

.fa-cell__no-pading .fa-cell__bd_padding .fa-form__input {
    height: 26px
}

.fa-cell__no-pading .fa-cell__ft_padding {
    padding: 12px 15px 12px 0
}

.fa-cell.last-cell::after {
    display: none
}

.fa-cell--access .fa-cell__ft {
    padding-right: 13px
}

.fa-cell--access .fa-cell__ft::after {
    position: absolute;
    top: 50%;
    right: 2px;
    content: " ";
    display: inline-block;
    height: 6px;
    width: 6px;
    border-width: 2px 2px 0 0;
    border-color: #c8c8c8;
    border-style: solid;
    -webkit-transform: translateY(-50%) matrix(.71, .71, -.71, .71, 0, 0);
    transform: translateY(-50%) matrix(.71, .71, -.71, .71, 0, 0)
}

.fa-cell--switch {
    padding-top: 6px;
    padding-bottom: 6px
}

/*uploader*/
.fa-uploader__hd {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    padding-bottom: 10px;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center
}

.fa-uploader__title {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1
}

.fa-uploader__info {
    color: #b2b2b2
}

.fa-uploader__bd {
    margin-bottom: -4px;
    margin-right: -9px;
    overflow: hidden
}

.fa-uploader__file {
    float: left;
    margin-right: 9px;
    margin-bottom: 9px
}

.fa-uploader__img {
    display: block;
    width: 79px;
    height: 79px
}

.fa-uploader__file_status {
    position: relative
}

.fa-uploader__file_status:before {
    content: " ";
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background-color: rgba(0, 0, 0, .5)
}

.fa-uploader__file-content {
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    color: #fff
}

.fa-uploader__input-box {
    float: left;
    position: relative;
    margin-right: 9px;
    margin-bottom: 9px;
    width: 77px;
    height: 77px;
    border: 1px solid #d9d9d9
}

.fa-uploader__input-box:after, .fa-uploader__input-box:before {
    content: " ";
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background-color: #d9d9d9
}

.fa-uploader__input-box:before {
    width: 2px;
    height: 39.5px
}

.fa-uploader__input-box:after {
    width: 39.5px;
    height: 2px
}

.fa-uploader__input-box:active {
    border-color: #999
}

.fa-uploader__input-box:active:after, .fa-uploader__input-box:active:before {
    background-color: #999
}

.fa-uploader__input {
    position: absolute;
    z-index: 1;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0
}
